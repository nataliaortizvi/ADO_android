        app:layout_constraintEnd_toEndOf="@+id/btnFvet"
        app:layout_constraintHorizontal_bias="0.55"
        app:layout_constraintStart_toStartOf="@+id/btnFvet"
        app:layout_constraintTop_toTopOf="@+id/btnFvet" />

</androidx.constraintlayout.widget.ConstraintLayout>