<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/C1fondo"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondoh"
    tools:context=".home">

    <TextView
        android:id="@+id/texto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginTop="32dp"
        android:fontFamily="@font/dmsans_bold"
        android:text="Fundaciones"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="0dp"
        android:layout_height="70dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent">

        <Button
            android:id="@+id/home"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginBottom="16dp"
            android:background="@drawable/house"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/perfil"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginEnd="64dp"
            android:layout_marginBottom="16dp"
            android:background="@drawable/usu"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/notif"
            android:layout_width="20dp"
            android:layout_height="25dp"
            android:layout_marginStart="64dp"
            android:layout_marginBottom="16dp"
            android:background="@drawable/camp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="100dp"
        app:layout_constraintBottom_toTopOf="@+id/constraintLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <ImageView
            android:id="@+id/btnMundo"
            android:layout_width="wrap_content"
            android:layout_height="400px"
            android:layout_marginTop="50dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/btnVet"
            app:srcCompat="@drawable/fotoe" />

        <ImageView
            android:id="@+id/btnSisben"
            android:layout_width="wrap_content"
            android:layout_height="400px"
            app:layout_constraintEnd_toStartOf="@+id/btnVet"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/fotoq" />

        <ImageView
            android:id="@+id/btnAmor"
            android:layout_width="wrap_content"
            android:layout_height="400px"
            android:layout_marginTop="50dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/btnMundo"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/btnSisben"
            app:srcCompat="@drawable/fotow" />

        <ImageView
            android:id="@+id/btnVet"
            android:layout_width="wrap_content"
            android:layout_height="400px"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/fotor" />

        <TextView
            android:id="@+id/texto1"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/dmsans_bold"

            android:text="Fundaci贸n Amor animal"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            app:layout_constraintBottom_toBottomOf="@+id/btnAmor"
            app:layout_constraintEnd_toStartOf="@+id/btnMundo"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/btnAmor" />

        <TextView
            android:id="@+id/texto3"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/dmsans_bold"
            android:text="Fundaci贸n Mundo Animal"

            android:textAlignment="center"
            android:textColor="#FFFFFF"
            app:layout_constraintBottom_toBottomOf="@+id/btnMundo"
            app:layout_constraintEnd_toEndOf="@+id/btnMundo"
            app:layout_constraintStart_toStartOf="@+id/btnMundo"
            app:layout_constraintTop_toTopOf="@+id/btnMundo" />

        <TextView
            android:id="@+id/texto4"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/dmsans_bold"
            android:text="Fundaci贸n veterinaria huellitas"

            android:textAlignment="center"
            android:textColor="#FFFFFF"
            app:layout_constraintBottom_toBottomOf="@+id/btnVet"
            app:layout_constraintEnd_toEndOf="@+id/btnVet"
            app:layout_constraintStart_toEndOf="@+id/btnSisben"
            app:layout_constraintTop_toTopOf="@+id/btnVet" />

        <TextView
            android:id="@+id/texto2"
            android:layout_width="100dp"
            android:layout_height="wrap_content"

            android:fontFamily="@font/dmsans_bold"
            android:text="Fundaci贸n Sisben para Perros y Gatos"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            app:layout_constraintBottom_toBottomOf="@+id/btnSisben"
            app:layout_constraintEnd_toEndOf="@+id/btnSisben"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/btnSisben" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>